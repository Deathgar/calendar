@model dynamic

@{
    ViewBag.Title = "Diagrams";
}

    <head>
        <script src="~/Scripts/Chart.min.js"></script>
       
    </head>

<body>
        <div>
            <div style="float: left;">
                <div class="diagramButtons" style="width: 200px; height: 400px; overflow: auto;  margin-top: 100px; margin-right: 40px; border: 1px double gray; background-color: #eff1f4; padding-left: 12px; padding-right: 12px; padding-top: 10px; padding-bottom: 10px;">
                   
                </div>
                <div style="text-align: center; margin-right: 40px; margin-top: 30px;">
                    <input id="Your" type="button" value="Your" style="width: 53px;  margin-right: 10px" />
                    <input id="Users" type="button" value="Users" style="width: 53px; margin-right: 10px"/>
                    <input id="All" type="button" value="All" style="width: 53px"/>
                    <input id="AllLinesUsers" type="button" value="All" style="width: 53px; display: none"/>
                </div>
                </div>

            <div style="float: left">
                <canvas id="speedChart" width="800" height="600"></canvas>
            </div>


        </div>
    </body>


@Scripts.Render("~/bundles/Diagrams")
<script type="text/javascript">
    $(document).ready(StartPage('@Url.Action("GetN","Diagrams")','@Url.Action("GetUsers","Diagrams")'))
</script>
